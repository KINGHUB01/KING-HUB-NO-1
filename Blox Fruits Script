local TestTab = Page:CreateSection({
    Name = "Combat", -- ชื่อ
    Side = "Right" -- ตำแหน่ง Left/Right
})
players = {}

for i,v in pairs(game:GetService("Players"):GetChildren()) do
   table.insert(players,v.Name)
end

TestTab:AddDropdown({
	Name = "Players",
	Value = players, -- ค่าที่จะให้มันเลือก
	List = players,
	Callback = function(abc)
    PlayersPPEED = abc
	end
})
TestTab:AddButton({
    Name = "Refresh",
    Callback = function()
    table.clear(players)
for i,v in pairs(game:GetService("Players"):GetChildren()) do
   table.insert(players,v.Name)
end
    end
})

TestTab:AddToggle({
    Name = "Spectate",
	Value = _G.ConfigToggle, -- ปรับค่าToggle true/false or Config
    Callback = function(bool)
			Sp = bool
			local plr1 = game.Players.LocalPlayer.Character.Humanoid
			local plr2 = game.Players:FindFirstChild(PlayersPPEED)
			repeat wait(.1)
				game.Workspace.Camera.CameraSubject = plr2.Character.Humanoid
			until Sp == false 
			game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
    end
})

TestTab:AddToggle({
    Name = "Teleport [close up]",
	Value = _G.ConfigToggle, -- ปรับค่าToggle true/false or Config
    Callback = function(TP)
_G.TPs = TP

    spawn(function()
       game:GetService("RunService").RenderStepped:Connect(function()
        pcall(function()
            if _G.TPs then
                setfflag("HumanoidParallelRemoveNoPhysics", "False")
                setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
                game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[PlayersPPEED].Character.HumanoidRootPart.CFrame * CFrame.new(0,20,0)
            end
        end)
       end)
    end)
    end
})

