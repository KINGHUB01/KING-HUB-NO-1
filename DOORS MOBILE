        getgenv().col = noclip
       for i,v in next, game.Players.LocalPlayer.Character:GetDescendants() do
          if v.IsA(v,"BasePart") and getgenv().col == false then
             v.CanCollide = true
          elseif v.IsA(v,"BasePart") and getgenv().col == true then
            v.CanCollide = false
          end
       end
end)
